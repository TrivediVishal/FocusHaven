<%= form_with(model: session) do |form| %>
  <% if session.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(session.errors.count, "error") %> prohibited this session from being saved:</h2>

      <ul>
        <% session.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :initial_time, style: "display: block" %>
    <%= form.datetime_field :initial_time %>
  </div>

  <div>
    <%= form.label :started_at, style: "display: block" %>
    <%= form.datetime_field :started_at %>
  </div>

  <div>
    <%= form.label :completed_at, style: "display: block" %>
    <%= form.datetime_field :completed_at %>
  </div>

  <div>
    <%= form.label :duation, style: "display: block" %>
    <%= form.number_field :duation %>
  </div>

  <div>
    <%= form.label :is_completed, style: "display: block" %>
    <%= form.check_box :is_completed %>
  </div>

  <div>
    <%= form.label :incomplete_id, style: "display: block" %>
    <%= form.text_field :incomplete_id %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
